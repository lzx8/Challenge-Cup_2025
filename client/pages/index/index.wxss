/* 悬浮按钮整体容器 */
.avatar-btn {
  position: fixed;
  bottom: 30px;
  right: 30px;
  width: 88px;
  height: 88px;
  border-radius: 50%;
  overflow: visible;                /* 让光晕不被裁切 */
  z-index: 999;
  animation: glow 3s ease-in-out infinite;
}

/* 头像本体（圆形裁剪） */
.avatar-img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  object-fit: cover;
}

/* 小手图标，做挥手动画 */
.hand-img {
  position: absolute;
  width: 32px;
  height: 32px;
  right: -4px;          /* 稍微探出头像外 */
  top: -4px;
  transform-origin: 70% 70%;   /* 旋转中心偏手腕 */
  animation: wave 2s ease-in-out infinite;
}

/* BOX‑SHADOW 光晕脉冲 */
@keyframes glow {
  0%   { box-shadow: 0 0 6px rgba(0,255,255,0.4); }
  50%  { box-shadow: 0 0 20px rgba(0,255,255,0.8); }
  100% { box-shadow: 0 0 6px rgba(0,255,255,0.4); }
}

/* 小手挥动：左右摇 20°
@keyframes wave {
  0%,100% { transform: rotate(0deg); }
  25%     { transform: rotate(-20deg); }
  75%     { transform: rotate(20deg); }
} */

